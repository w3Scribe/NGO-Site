      "@pages/*": ["./pages/*"],
      "@utils": ["./utils"],
      "@utils/*": ["./utils/*"],
      "@assets": ["./assets"],
      "@assets/*": ["./assets/*"],
      "@public": ["./public"],
      "@public/*": ["./public/*"]
    }
  }
}
